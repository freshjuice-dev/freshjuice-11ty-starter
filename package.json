{
  "name": "freshjuice-11ty-starter",
  "version": "1.0.0",
  "description": "FreshJuice 11ty starter theme with CloudCannon CMS integration",
  "type": "module",
  "scripts": {
    "setup:local": "npm install --no-save puppeteer @axe-core/puppeteer eslint prettier favicons serve-handler rimraf",
    "clean": "rimraf ./_site ./.cache",
    "watch:11ty": "eleventy --serve --quiet",
    "build:11ty": "eleventy",
    "watch:js": "esbuild ./src/assets/js/main.js --outfile=./_site/assets/js/main.js --bundle --watch --sourcemap",
    "build:js": "esbuild ./src/assets/js/main.js --outfile=./_site/assets/js/main.js --bundle --minify",
    "watch:css": "npx @tailwindcss/cli -i ./src/assets/css/main.css -o ./_site/assets/css/main.css --watch",
    "build:css": "npx @tailwindcss/cli -i ./src/assets/css/main.css -o ./_site/assets/css/main.css --minify",
    "start": "ELEVENTY_ENV=development npm-run-all --parallel watch:*",
    "dev": "npm run start",
    "build": "ELEVENTY_ENV=production NODE_ENV=production npm-run-all --serial build:js build:css build:11ty postbuild",
    "postbuild": "npm-run-all --parallel postbuild:*",
    "postbuild:pagefind": "pagefind --site ./_site",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "npm run test:a11y",
    "test:a11y": "node tests/a11y.test.mjs",
    "test:a11y:live": "node tests/a11y.test.mjs --sitemap=https://snappy-lemon-starter.freshjuice.dev/sitemap.xml --base-url=https://snappy-lemon-starter.freshjuice.dev",
    "favicon": "node scripts/generate-favicons.mjs"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "starter",
    "cloudcannon",
    "tailwindcss"
  ],
  "author": {
    "name": "FreshJuice",
    "email": "contact@freshjuice.dev",
    "url": "https://freshjuice.dev/"
  },
  "contributors": [
    {
      "name": "Alex Zappa",
      "email": "alex@zappa.dev",
      "url": "https://alex.zappa.dev/"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@alpinejs/collapse": "^3.15.3",
    "@alpinejs/intersect": "^3.15.3",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@zachleat/table-saw": "^1.0.7",
    "alpinejs": "^3.15.3",
    "eleventy-plugin-phosphoricons": "^2.0.0",
    "eleventy-plugin-speculation-rules": "^2.0.0",
    "esbuild": "^0.27.2",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.1",
    "linkedom": "^0.18.12",
    "lite-youtube-embed": "^0.3.4",
    "npm-run-all": "^4.1.5",
    "pagefind": "^1.4.0",
    "rimraf": "^6.1.2",
    "shiki": "^3.21.0",
    "tailwindcss": "^4.1.18",
    "wrangler": "^4.58.0"
  },
  "dependencies": {
    "@bookshop/browser": "3.17.1",
    "@bookshop/eleventy-bookshop": "3.17.1"
  }
}
